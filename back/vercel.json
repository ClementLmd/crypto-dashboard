{
  "version": 2,
  "buildCommand": "cd ../shared && pnpm build && cd ../back && pnpm install && pnpm build",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/api/index.ts" }],
  "env": {
    "NODE_ENV": "production"
  }
}
