{
  "version": 2,
  "installCommand": "pnpm install && pnpm -w run build-shared",
  "buildCommand": "tsc --noEmit false --skipLibCheck true && cp package.json dist/",
  "nodeVersion": "22.x",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
